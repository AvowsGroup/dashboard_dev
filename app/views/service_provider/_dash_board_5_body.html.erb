             <script type="text/javascript">
        //load the Google Visualization API and the chart
        google.load('visualization', '1', {'packages':['columnchart']});

        //set callback
        google.setOnLoadCallback (createChart);

        //callback function
        function createChart() {

                        var data = new google.visualization.DataTable(
                                   {
                                  "cols": [
                                        {"id":"a","label":"Topping","pattern":"","type":"string"},
                                        {"id":"v","label":"Promedio","pattern":"","type":"number"}
                                      ],
                                  "rows": [
                                        {"c":[{"v":"<1", "f":null},{"v":<%=@doctorquotausagezero%>,"f":null}]},
                                        {"c":[{"v":"1-100", "f":null},{"v":<%=@doctorquotausage1to100%>,"f":null}]},
                                        {"c":[{"v":"101-200", "f":null},{"v":<%=@doctorquotausage101to200%>,"f":null}]},
                                        {"c":[{"v":"201-300", "f":null},{"v":<%=@doctorquotausage201to300%>,"f":null}]},
                                        {"c":[{"v":"301-400", "f":null},{"v":<%=@doctorquotausage301to400%>,"f":null}]},
                                                 {"c":[{"v":"401-500", "f":null},{"v":<%=@doctorquotausage401to500%>,"f":null}]},
                                                 {"c":[{"v":"501-600", "f":null},{"v":<%=@doctorquotausage501to600%>,"f":null}]},
                                                 {"c":[{"v":"601-700", "f":null},{"v":<%=@doctorquotausage601to700%>,"f":null}]},
                                                 {"c":[{"v":"701-800", "f":null},{"v":<%=@doctorquotausage701to800%>,"f":null}]},                                              
                                
                                      ]
                                }
                        )

            //instantiate our chart objects
            var chart = new google.visualization.ColumnChart (document.getElementById('chart_div'));
                        
                        
            //define options for visualization
            var options = {
               
                                width: 350,
                height: 218,
                is3D: true,
                legend: 'none',                        
                                title: '',

                                 hAxis: {
                                        title: 'Quota Use',
                                        
                                        textPosition: 'out'
                                },
                                                
                                vAxis: {
                                        title: 'Doctors',
                                        minValue: 0,
                                        maxValue: 10
                                
                                }
            };
             chart
                    .tooltip()
                    .position('center-top')
                    .anchor('center-bottom')
                    .offsetX(0)
                    .offsetY(5)
                    .format('{%Value}');

            //draw my chart
           chart.draw(data, options);

        }

    </script>
                         
